<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
<appSettings>
    <add key="QueueName" value=".\private$\ShippingQueue" />
  </appSettings>
  <system.serviceModel>
    <services>
      <!-- 1. service details-->
      <service name="AG.SendMailService">
        <!-- 2.  endpoint details-->
        <endpoint address="msmq.formatname:DIRECT=OS:.\private$\ShippingQueue" binding="msmqIntegrationBinding" bindingConfiguration="IMHBinding" contract="AG.ISendMail">
        </endpoint>
      </service>      
    </services>
  <!-- 3. binding details-->
  <bindings>
      <msmqIntegrationBinding>
        <binding name="IMHBinding"
                 closeTimeout="00:30:00"
                 receiveTimeout="01:00:00"
                 retryCycleDelay="00:00:10"
                 receiveRetryCount="0"
                 exactlyOnce="true"
                 maxRetryCycles="3"
                 receiveErrorHandling="Move">
          <security mode="None"/>
        </binding>
      </msmqIntegrationBinding>
    </bindings>
  </system.serviceModel>
<!--4. Trace configuration-->
<system.diagnostics>
    <trace autoflush="true" indentsize="4">
      <listeners>
        <add name="myListener" 
          type="System.Diagnostics.TextWriterTraceListener"
             initializeData="WrittenTraceLog.log"/>
        <remove name="Default"></remove>
      </listeners>
    </trace>
  </system.diagnostics>
</configuration>